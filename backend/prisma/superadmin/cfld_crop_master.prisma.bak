// FLD crop master - models only; generator/datasource in schema.prisma

model FLDCropMaster {
  cfldId   Int    @id @default(autoincrement()) @map("cfld_id")
  Season   String @map("season")
  Type     String @map("type")
  CropName String @map("crop_name")

  @@map("fld_crop_master")
  @@index([Season])
  @@index([Type])
  @@index([CropName])
}
