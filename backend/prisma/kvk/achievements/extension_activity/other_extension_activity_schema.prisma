model KvkOtherExtensionActivity {
  otherExtensionActivityId Int @id @default(autoincrement()) @map("kvk_other_extension_activity_id")

  kvkId Int
  kvk   Kvk @relation(fields: [kvkId], references: [kvkId])

  fldId Int?
  fld   KvkFldIntroduction? @relation(fields: [fldId], references: [kvkFldId])

  staffId Int?
  staff   KvkStaff? @relation(fields: [staffId], references: [kvkStaffId])

  activityTypeId Int?
  activityType   OtherExtensionActivityType? @relation(fields: [activityTypeId], references: [activityTypeId])

  numberOfActivities Int @map("number_of_activities")
  startDate DateTime @map("start_date")
  endDate   DateTime @map("end_date")

  @@index([kvkId])
  @@index([fldId])
  @@index([staffId])
  @@index([activityTypeId])

  @@map("kvk_other_extension_activity")
}