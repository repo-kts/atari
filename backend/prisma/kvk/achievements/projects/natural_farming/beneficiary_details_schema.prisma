model BeneficiariesDetails {
  beneficiariesDetailsId Int @id @default(autoincrement()) @map("beneficiaries_details_id")

  kvkId Int
  kvk   Kvk @relation(fields: [kvkId], references: [kvkId])

  year Int
  blocksCovered  Int @map("blocks_covered")
  villagesCovered Int @map("villages_covered")
  totalTrainedFarmers Int @map("total_trained_practicing_nf_farmers")
  farmersInfluenced Int @map("farmers_influenced")
  farmersEngagedAllSeason Int @map("farmers_engaged_all_season")
  farmersEngagedOneSeason Int @map("farmers_engaged_one_season")
  remarks String @map("remarks")

  @@index([kvkId])

  @@map("beneficiaries_details")
}
